a, b, c = 20, -33, 48  
discriminant = b**2 - 4*a*c
if discriminant > 0:
    root1 = (-b + discriminant**0.5) / (2 * a)
    root2 = (-b - discriminant**0.5) / (2 * a)
    print(f"Roots are real and distinct: {root1}, {root2}")
elif discriminant == 0:
    root = -b / (2 * a)
    print(f"Roots are real and identical: {root}")
else:
    print("Roots are complex.")
